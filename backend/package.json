{
  "name": "mystery-link-backend",
  "version": "1.0.0",
  "description": "Cloudflare Workers backend for Mystery Link multiplayer game",
  "main": "src/index.ts",
  "scripts": {
    "build": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "dev": "wrangler dev",
    "dev:local": "wrangler dev --local",
    "tail": "wrangler tail",
    "test": "tsx test/load_test.ts",
    "test:load": "tsx test/load_test.ts --players 100 --messages 10"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "websocket",
    "multiplayer",
    "game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "wrangler": "^3.19.0",
    "ws": "^8.14.2"
  }
}
